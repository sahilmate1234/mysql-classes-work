--  -- Table 4 Salaries ----------------------------------------------------
create table Salaries (
employee_id int  , 
employee_name varchar(100) ,
overtime  time , 
Date  date  , 
salary  int , 
dept_id int  , 
salary_type  varchar(100),
payment_id int   , 
leaves  varchar(10)   ,
status varchar(50) ,
foreign key (employee_id ) references Employees (ID) ,
foreign key (dept_id ) references Departments(dept_id) 
);

