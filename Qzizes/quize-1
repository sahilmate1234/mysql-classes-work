--  --- Table 2 Departments
create table Departments(
dept_id int primary key , 
dept_name varchar(50) , 
head_of_dept varchar(15) ,
total_employes int , 
address  varchar(100),
projects varchar(100) ,
section varchar(10) ,
Decription  varchar(100) ,
status  varchar(100), 
location varchar(50)
);
