-- Table 5 Clients
create table Clients (
 id  int primary key  , 
 name varchar(100) ,
 age  int , 
 address varchar(100) ,
 email  varchar(100), 
 contact varchar (100), 
 status  varchar(100)  ,
 payment  varchar(60),
 project_id int ,
 foreign key( project_id) references Projects( project_id)
 );
 
