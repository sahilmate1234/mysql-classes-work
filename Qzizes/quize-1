--  --- Table 3 Projects --
create table Projects (
project_id int primary key , 
project_name varchar(50) ,
Head_of_project varchar(100) , 
start_date datetime , 
end_date  datetime ,
client_requirement varchar(100),
cost_of_project  int  ,
total_employes int  ,
Decription varchar(100),
status varchar(100)
 );
 
